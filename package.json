{
  "name": "@rakuten-rex/text-field",
  "version": "1.1.3",
  "description": "TextField component",
  "main": "src/index.jsx",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development ./node_modules/.bin/start-storybook -p 8080 --ci -s ./.storybook/public",
    "build": "npm run build:component && npm run build:storybook",
    "build:patch": "npm ci && npm --no-git-tag-version version patch && npm run build:commit && npm run publish:component",
    "build:minor": "npm ci && npm --no-git-tag-version version minor && npm run build:commit && npm run publish:component",
    "build:major": "npm ci && npm --no-git-tag-version version major && npm run build:commit && npm run publish:component",
    "build:storybook": "NODE_ENV=production ./node_modules/.bin/build-storybook -c .storybook -o docs -s ./npm,./.storybook/public",
    "build:component": "NODE_ENV=production ./node_modules/.bin/webpack --mode=production --config webpack.config.js",
    "build:commit": "npm run build && npm run commit",
    "commit": "npm run commit:docs && npm run commit:npm && npm run commit:package",
    "commit:docs": "git add docs README.md && git commit -m 'Added - Docs for github pages and README file'",
    "commit:npm": "git add npm && git commit -m 'Added - NPM distribution'",
    "commit:package": "git add package.json package-lock.json && git commit -m 'New version - v'$npm_package_version",
    "serve": "NODE_ENV=production ./node_modules/.bin/http-server -p 8081 docs",
    "publish:component": "cd ./npm && npm publish",
    "setup": "npm ci && npm run generate:package-json && npm run generate:rex-component",
    "generate:package-json": "plop --plopfile project-scripts/plop-templates/package-json/plop-generator.js",
    "generate:rex-component": "plop --plopfile project-scripts/plop-templates/RexComponent/plop-generator.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rakuten-rex/text-field.git"
  },
  "author": "ReX React UI Components Library Authors (https://github.com/rakuten-rex/text-field/graphs/contributors)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rakuten-rex/text-field/issues"
  },
  "homepage": "https://rakuten-rex.github.io/text-field/",
  "dependencies": {
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@rakuten-rex/eslint-config-rex-react-component": "1.5.1",
    "@rakuten-rex/storybook-inspect-html": "1.3.2",
    "@rakuten-rex/storybook-inspect-jsx": "1.3.4",
    "@storybook/addon-a11y": "5.2.8",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-backgrounds": "5.2.8",
    "@storybook/addon-knobs": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addon-storysource": "5.2.8",
    "@storybook/addon-viewport": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react": "5.2.8",
    "@svgr/webpack": "4.3.3",
    "acorn": "7.1.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-add-react-displayname": "0.0.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "bundle-scss": "1.4.10",
    "copy-webpack-plugin": "5.0.5",
    "core-js": "3.4.1",
    "css-loader": "3.2.1",
    "cssnano": "4.1.10",
    "del": "5.1.0",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react-hooks": "1.7.0",
    "fibers": "4.0.2",
    "file-loader": "4.2.0",
    "fsevents": "2.1.2",
    "glob": "7.1.6",
    "google-closure-compiler-js": "20180610.0.0",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "mini-css-extract-plugin": "0.8.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "plop": "2.5.3",
    "postcss-css-variables": "0.14.0",
    "prettier": "1.19.1",
    "sass": "1.23.7",
    "sass-loader": "8.0.0",
    "strip-comments": "2.0.1",
    "style-loader": "1.0.1",
    "typescript": "3.5.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-merge": "4.2.2"
  },
  "rex-starter-kit": "3.4.8"
}
